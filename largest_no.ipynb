def largest_number(numbers: list) -> int:
    """
    args:
        Take a list of numbers as input
    Returns:
        returns the largest number using loop
    """
    if not isinstance(numbers, list):
        raise TypeError("Input list honi chahiye")

    if len(numbers) == 0:
        raise ValueError("List empty nahi honi chahiye")

    for num in numbers:
        if not isinstance(num, (int, float)):
            raise TypeError("List ke andar sirf numbers hone chahiye")

    largest = numbers[0]
    for num in numbers:
        if num > largest:
            largest = num   # loop logic

    return largest


if __name__ == "__main__":
    try:
        output = largest_number([10, 45, 3, 99, 23, 67])
        print("Largest Number:", output)
    except (TypeError, ValueError) as e:
        print("Error:", e)
    except Exception as e:
        print(e)

    print("hello world")

